<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SportsDashboard.Styles">
    
    <Color x:Key="WhiteBackground">#ffffff</Color>
    <SolidColorBrush x:Key="WhiteBackgroundBrush" Color="{StaticResource WhiteBackground}"/>

    <Color x:Key="LightGreyBackground">#eeeeee</Color>
    <SolidColorBrush x:Key="LightGreyBackgroundBrush" Color="{StaticResource LightGreyBackground}"/>

    <Color x:Key="GreyBackground">#7E8889</Color>
    <SolidColorBrush x:Key="GreyBackgroundBrush" Color="{StaticResource GreyBackground}"/>

    <Color x:Key="DarkGreyBackground">#212121</Color>
    <SolidColorBrush x:Key="DarkGreyBackgroundBrush" Color="{StaticResource DarkGreyBackground}"/>

    <Color x:Key="TwitchDeepPurple">#26183f</Color>
    <SolidColorBrush x:Key="TwitchDeepPurpleBrush" Color="{StaticResource TwitchDeepPurple}"/>

    <Color x:Key="TwitchLightPurple">#4b307b</Color>
    <SolidColorBrush x:Key="TwitchLightPurpleBrush" Color="{StaticResource TwitchLightPurple}"/>

    



</ResourceDictionary>